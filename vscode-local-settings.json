{
  "workbench.colorTheme": "Go - Playground",
  "files.autoSave": "onFocusChange",
  "editor.fontFamily": "Inconsolata LGC, Menlo, Monaco, 'Courier New', monospace",
  "editor.tabSize": 8,
  "editor.wordWrap": "on",
  "editor.autoClosingBrackets": "never",
  "editor.autoClosingQuotes": "never",
  "editor.autoIndent": "keep",
  "editor.folding": false,
  "editor.links": false,
  "editor.renderIndentGuides": false,
  "editor.roundedSelection": false,
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.minimap.enabled": false,
  "files.simpleDialog.enable": true,
  "terminal.external.osxExec": "iTerm2.app",
  "terminal.integrated.shell.osx": "/bin/zsh",
  "terminal.integrated.env.osx": {
    "TMUX_AUTOSTART": "1",
    "TMUX_VSCODE_SOCK": "1"
  },
  "extensions.ignoreRecommendations": true,
  "terminal.integrated.fontFamily": "Menlo",
  "editor.insertSpaces": false,
  "editor.acceptSuggestionOnEnter": "smart",
  "git.autofetch": true,
  "git.confirmSync": false,
  "workbench.startupEditor": "none",
  "editor.wrappingIndent": "none",
  "terminal.integrated.macOptionIsMeta": true,
  "window.zoomLevel": 0,
  "selectline.statusbarPriority": 1,
  "selectline.displayFormat": "%d lines selected",
  "go.buildOnSave": "off",
  "go.useLanguageServer": true,
  "[go]": {
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    },
    "editor.snippetSuggestions": "none",
    "editor.codeActionsOnSaveTimeout": 3000
  },
  "gopls": {
    "usePlaceholders": true,
    "staticcheck": false
  },
  "python.terminal.activateEnvironment": false,
  "python.linting.flake8Enabled": true,
  "python.formatting.provider": "black",
  "python.linting.mypyEnabled": true,
  "terminal.integrated.automationShell.osx": "bash",
  "tlaplus.java.options": "-DTLA-Library=/home/klepa/.local/share/tla/mods",
  "svgviewer.transparencycolor": "#f8de7e",
  "editor.lineNumbers": "off",
  "terminal.integrated.inheritEnv": false,
  "editor.parameterHints.enabled": false,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "java.errors.incompleteClasspath.severity": "ignore",
  "terminal.integrated.rendererType": "dom",
  "java.maven.downloadSources": true,
  "java.maven.updateSnapshots": true,
  "workbench.iconTheme": null,
  "editor.glyphMargin": false
}