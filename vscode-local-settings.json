{
    "workbench.colorTheme": "Go - Playground",
    "files.autoSave": "onFocusChange",
    "editor.fontFamily": "Inconsolata LGC, Menlo, Monaco, 'Courier New', monospace",
    "editor.tabSize": 8,
    "editor.wordWrap": "on",
    "editor.autoClosingBrackets": "never",
    "editor.autoClosingQuotes": "never",
    "editor.autoIndent": false,
    "editor.codeActionsOnSaveTimeout": 2000,
    "editor.folding": false,
    "editor.glyphMargin": false,
    "editor.lineNumbers": "off",
    "editor.links": false,
    "editor.renderIndentGuides": false,
    "editor.roundedSelection": false,
    "editor.formatOnSave": true,
    "editor.formatOnSaveTimeout": 2000,
    "editor.formatOnPaste": true,
    "editor.minimap.enabled": false,
    "files.simpleDialog.enable": true,
    "terminal.external.osxExec": "iTerm2.app",
    "terminal.integrated.shell.osx": "/usr/local/bin/zsh",
    "terminal.integrated.env.osx": {
        "TMUX_AUTOSTART": "1",
        "TMUX_VSCODE_SOCK": "1"
    },
    "extensions.ignoreRecommendations": true,
    "terminal.integrated.fontFamily": "Menlo",
    "editor.insertSpaces": false,
    "editor.acceptSuggestionOnEnter": "smart",
    "git.autofetch": true,
    "git.confirmSync": false,
    "workbench.startupEditor": "none",
    "editor.wrappingIndent": "none",
    "terminal.integrated.macOptionIsMeta": true,
    "window.zoomLevel": 0,
    "selectline.statusbarPriority": 1,
    "selectline.displayFormat": "%d lines selected",
    "go.formatTool": "goimports",
    "python.terminal.activateEnvironment": false,
    "python.linting.flake8Enabled": true,
    "python.formatting.provider": "black",
    "python.linting.mypyEnabled": true,
    "terminal.integrated.automationShell.osx": "bash",
    "go.buildOnSave": "off",
}